#source 'https://github.com/CocoaPods/Specs.git'
source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'

platform :ios, '11.0'

target 'MHAdSDKDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MHAdSDKDemo
  
  # ----- -----
  # 必须 - MH 广告SDK
  #推荐通过远程cocoapods方式直接接入。
  pod 'MHAdSDK', '~> 1.3.5.4'
  
  # 如果需要本地cocoapods方式接入，请先下载  
  # http://static.maplehaze.cn/sdk/ios/release/package/mh_adsdk_v1.3.5.4.zip
  # 将SDK文件夹放入工程，再使用下面的本地路径引入
  # pod 'MHAdSDK', :path => './MHAdSDK'
  
  # 以下是依赖的各个平台的SDK
  # 优量汇
  pod 'GDTMobSDK', '~> 4.15.40'
  # 爱奇艺
  pod 'iAdSDK', '~> 1.18.002'
  # jdsdk
  #pod 'JADYun'
  #pod 'JADYunMotion'

  
end

